{
    "name": "jpmschuler\/typo3-extdev-helper",
    "license": "MIT",
    "type": "project-configuration",
    "authors": [
        {
            "name": "J. Peter M. Schuler",
            "email": "j.peter.m.schuler@uni-due.de"
        }
    ],
    "require": {
        "bk2k\/configuration-installer": "*@dev",
        "ergebnis\/composer-normalize": "^2.15",
        "friendsofphp\/php-cs-fixer": "^v3.1.0",
        "helmich\/typo3-typoscript-lint": "^2.5",
        "micheh\/phpcs-gitlab": "^1.1.0",
        "michielroos\/typo3scan": "^1.7.3",
        "nimut\/testing-framework": "^6",
        "saschaegerer\/phpstan-typo3": "^1",
        "sebastian\/phpcpd": "^6.0.3",
        "squizlabs\/php_codesniffer": "^3.6",
        "ssch\/typo3-rector": "^1",
        "typo3\/tailor": "^1.5",
        "ext-json": "*"
    },
    "require-dev": {
        "roave\/security-advisories": "dev-latest",
        "typo3\/cms-core": "^v11.4.16"
    },
    "config": {
        "allow-plugins": {
            "bk2k\/configuration-installer": true,
            "ergebnis\/composer-normalize": true,
            "typo3\/class-alias-loader": true,
            "typo3\/cms-composer-installers": true
        },
        "bin-dir": ".Build\/bin",
        "sort-packages": true,
        "vendor-dir": ".Build\/vendor"
    },
    "extra": {
        "typo3\/cms": {
            "app-dir": ".Build",
            "web-dir": ".Build\/public"
        }
    }
}
